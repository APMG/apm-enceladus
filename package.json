{
  "name": "@apmg/enceladus",
  "description": "A React component that takes an array of Image objects from our American Public Media's Images API and turns it into a slideshow, with some options",
  "version": "0.5.5",
  "main": "dist/bundle.js",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/APMG/apm-enceladus"
  },
  "keywords": [
    "image",
    "react",
    "apm",
    "media"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --watch",
    "test:ci": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "eslint": "eslint .",
    "prettier": "prettier --check '**/**.js'",
    "prettier:fix": "prettier --check '**/**.js' --write",
    "clean": "rm -rf node_modules dist package-lock.json && npm cache verify"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint && npm run prettier",
      "pre-push": "npm run eslint && npm run prettier"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "@apmg/mimas": "^0.3.0",
    "classnames": "^2.2.6",
    "next": "^9.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@apmg/mimas": "^0.3.0",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-options": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-prop-type-error": "^1.1.0",
    "mini-css-extract-plugin": "^0.6.0",
    "next": "^9.0.0",
    "node-sass": "^4.12.0",
    "prettier": "1.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-testing-library": "^5.8.0",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "sass-loader": "^7.1.0",
    "storybook-react-router": "^1.0.5",
    "storybook-readme": "^5.0.5",
    "uuid": "^3.3.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@emotion/core": "^10.0.16",
    "@emotion/styled": "^10.0.15",
    "focus-trap-react": "^6.0.0",
    "react-pose": "^4.0.8"
  }
}
